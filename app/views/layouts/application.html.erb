<!DOCTYPE html>
<html>
<head>
  <title>BulletinBoard</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
    <%= link_to "Logout", :logout %>
<% else %>
    <%= link_to "Log in", :login %>
<% end %>
<%= yield %>

</body>
</html>
